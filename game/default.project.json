{
	"name": "flamework-game",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"MaterialService": {
			"$className": "MaterialService",
			"$properties": {
				"Use2022Materials": true
			}
		},
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"TS": {
				"$path": "out/server"
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					},
					"@flamework": {
						"$path": "node_modules/@flamework"
					}
				}
			},
			"TS": {
				"$path": "out/shared"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"TS": {
					"$path": "out/client"
				}
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			},
			"map": {
				"$path": "assets/map.rbxm"
			}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		},
		"ServerStorage": {
			"$className": "ServerStorage",
			"assets": {
				"$className": "Folder",
				"terrain": {
					"$path": "assets/terrain.rbxm"
				}
			}
		},
		"Lighting": {
			"$className": "Lighting",
			"$properties": {
				"Ambient": [0.27, 0.27, 0.27],
				"Brightness": 3,
				"GlobalShadows": true,
				"OutdoorAmbient": [0.27, 0.27, 0.27],
				"TimeOfDay": "12:00:00",
				"ShadowSoftness": 0.2,
				"Technology": "ShadowMap",
				"GeographicLatitude": 0
			},
			"$path": "assets/lighting"
		}
	}
}
