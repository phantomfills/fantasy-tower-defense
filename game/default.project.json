{
	"name": "fantasy-tower-defense",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"MaterialService": {
			"$className": "MaterialService",
			"$properties": {
				"Use2022MaterialsXml": {
					"Bool": true
				}
			}
		},
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"TS": {
				"$path": "out/server"
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					},
					"@flamework": {
						"$path": "node_modules/@flamework"
					}
				}
			},
			"assets": {
				"$className": "Folder",
				"enemies": {
					"$className": "Folder",
					"dummy": {
						"$className": "Folder",
						"models": {
							"$className": "Folder",
							"dummy": {
								"$path": "assets/dummy.rbxm"
							}
						}
					}
				},
				"towers": {
					"$className": "Folder",
					"archer": {
						"$className": "Folder",
						"models": {
							"$className": "Folder",
							"level_0": {
								"$path": "assets/archer/level-0.rbxm"
							},
							"level_1": {
								"$path": "assets/archer/level-1.rbxm"
							},
							"archer": {
								"$path": "assets/archer.rbxm"
							}
						}
					}
				}
			},
			"TS": {
				"$path": "out/shared"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"TS": {
					"$path": "out/client"
				}
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			},
			"gameMap": {
				"$path": "assets/map.rbxm"
			}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		},
		"ServerStorage": {
			"$className": "ServerStorage"
		},
		"Lighting": {
			"$className": "Lighting",
			"$properties": {
				"Ambient": [0.27, 0.27, 0.27],
				"Brightness": 3,
				"GlobalShadows": true,
				"OutdoorAmbient": [0.27, 0.27, 0.27],
				"TimeOfDay": "12:00:00",
				"ShadowSoftness": 0.2,
				"Technology": "ShadowMap",
				"GeographicLatitude": 0,
				"EnvironmentDiffuseScale": 1,
				"EnvironmentSpecularScale": 1
			},
			"$path": "assets/lighting"
		}
	}
}
