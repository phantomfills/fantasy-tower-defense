{
	"name": "fantasy-tower-defense",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"MaterialService": {
			"$className": "MaterialService",
			"$properties": {
				"Use2022MaterialsXml": {
					"Bool": true
				}
			}
		},
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"TS": {
				"$path": "out/server"
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					},
					"@flamework": {
						"$path": "node_modules/@flamework"
					}
				}
			},
			"assets": {
				"$className": "Folder",
				"maps": {
					"$path": "assets/maps"
				},
				"projectiles": {
					"$className": "Folder",
					"boulder": {
						"$path": "assets/boulder.rbxm"
					}
				},
				"enemies": {
					"$className": "Folder",
					"models": {
						"$className": "Folder",
						"trainingDummy": {
							"$path": "assets/training-dummy.rbxm"
						},
						"armoredDummy": {
							"$path": "assets/armored-dummy.rbxm"
						},
						"speedsterDummy": {
							"$path": "assets/speedster-dummy.rbxm"
						},
						"stealthDummy": {
							"$path": "assets/stealth-dummy.rbxm"
						},
						"dummyTank": {
							"$path": "assets/dummy-tank.rbxm"
						},
						"guardDummy": {
							"$path": "assets/guard-dummy.rbxm"
						},
						"multiplierDummy": {
							"$path": "assets/multiplier-dummy.rbxm"
						},
						"dividedDummy": {
							"$path": "assets/divided-dummy.rbxm"
						},
						"korblox_deathspeaker": {
							"$path": "assets/korblox-deathspeaker.rbxm"
						},
						"circuit_breaker": {
							"$path": "assets/circuit-breaker.rbxm"
						},
						"impostor": {
							"$path": "assets/impostor.rbxm"
						},
						"critical_sports_car": {
							"$path": "assets/critical_sports_car.rbxm"
						}
					}
				},
				"towers": {
					"$className": "Folder",
					"archer": {
						"$className": "Folder",
						"models": {
							"$className": "Folder",
							"level_0": {
								"$path": "assets/archer/level-0.rbxm"
							},
							"level_1": {
								"$path": "assets/archer/level-1.rbxm"
							},
							"level_2": {
								"$path": "assets/archer/level-2.rbxm"
							},
							"level_3": {
								"$path": "assets/archer/level-3.rbxm"
							},
							"level_4": {
								"$path": "assets/archer/level-4.rbxm"
							},
							"level_5": {
								"$path": "assets/archer/level-5.rbxm"
							}
						}
					},
					"dummy_defect": {
						"$className": "Folder",
						"models": {
							"$className": "Folder",
							"level_0": {
								"$path": "assets/dummy-defect/level-0.rbxm"
							},
							"level_1": {
								"$path": "assets/dummy-defect/level-1.rbxm"
							},
							"level_2": {
								"$path": "assets/dummy-defect/level-2.rbxm"
							},
							"level_3": {
								"$path": "assets/dummy-defect/level-3.rbxm"
							},
							"level_4": {
								"$path": "assets/dummy-defect/level-4.rbxm"
							},
							"level_5": {
								"$path": "assets/dummy-defect/level-5.rbxm"
							}
						}
					}
				}
			},
			"TS": {
				"$path": "out/shared"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"$properties": {
				"CameraMinZoomDistance": 10,
				"CameraMaxZoomDistance": 40,
				"EnableMouseLockOption": false,
				"CharacterUseJumpPower": false,
				"CharacterJumpHeight": 7.2
			},
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"TS": {
					"$path": "out/client"
				}
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		},
		"ServerStorage": {
			"$className": "ServerStorage"
		},
		"Lighting": {
			"$className": "Lighting",
			"$properties": {
				"Ambient": [0.27, 0.27, 0.27],
				"Brightness": 3,
				"GlobalShadows": true,
				"OutdoorAmbient": [0.27, 0.27, 0.27],
				"TimeOfDay": "12:00:00",
				"ShadowSoftness": 0.2,
				"Technology": "ShadowMap",
				"GeographicLatitude": 0,
				"EnvironmentDiffuseScale": 1,
				"EnvironmentSpecularScale": 1
			},
			"$path": "assets/lighting"
		},
		"TextChatService": {
			"$className": "TextChatService",
			"$properties": {
				"ChatVersion": "TextChatService",
				"CreateDefaultCommands": true,
				"CreateDefaultTextChannels": true
			}
		}
	}
}
